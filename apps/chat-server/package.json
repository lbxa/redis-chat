    "@nestjs/testing": "^11.0.1",
    "@rspack/cli": "^1.3.9",
    "@rspack/core": "^1.3.9",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bun": "latest",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.18.1",
